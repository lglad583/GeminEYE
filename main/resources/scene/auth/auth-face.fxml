<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="vision.gemineye.controllers.AuthFaceController">
    <children>
        <VBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              HBox.hgrow="NEVER" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Feature Still In Progress.."
                       VBox.vgrow="ALWAYS"/>
                <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                    <children>
                        <Label onMouseClicked="#toTextUnlock" text="Use Credentials Instead">
                            <effect>
                                <DropShadow/>
                            </effect>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets top="100.0"/>
                    </VBox.margin>
                </HBox>
            </children>
            <HBox.margin>
                <Insets left="20.0"/>
            </HBox.margin>
            <VBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </VBox.margin>
        </VBox>
    </children>
</VBox>
